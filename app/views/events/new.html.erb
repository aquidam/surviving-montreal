<%= form_with model: @event do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :event_date %>
  <%= f.text_field :event_date %>
  <%= f.label :location %>
  <%= f.number_field :location %>
  <%= f.label :number_of_people %>
  <%= f.number_field :number_of_people %>
  <%= f.submit %>
<% end %>
