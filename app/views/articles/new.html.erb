<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :video_url %>
  <%= f.number_field :video_url %>
  <%= f.label :image_url %>
  <%= f.number_field :image_url %>
  <%= f.submit %>
<% end %>
